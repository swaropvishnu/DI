@model DI.Models.CMYSS_Applicant
@{
    ViewBag.Title = "MYSY_SchemeForm";
    Layout = "~/Views/Shared/UserLayout.cshtml";
    if (UserSession.LoggedInUserLevelId == null || UserSession.LoggedInUserLevelId != "30")
    {
        Response.Redirect("../Login/Registration_Login");
        return;
    }
}

<style>
    
    .disabled {
        pointer-events: none !important;
        cursor: default !important;
    }
</style>
<script>
    
    $(document).ready(function () {
        $("#txtDOB").datepicker({
            dateFormat: "dd/mm/yy",
            changeMonth: true,
            changeYear: true
            
        });
        //$("#txtDOB").datepicker({ changeMonth: true, changeYear: true, dateFormat: 'dd/mm/yy', minDate: '0d', showButtonPanel: true });
        $(".select2").select2();

        $('.select2-container--bootstrap').removeAttr('style');
        $('.select2-container--bootstrap').css('width', '100%');
        //$('.select2-container--below').removeAttr('style');
        //$('.select2-container--bootstrap').css('width', '100%');
    });
    $(document).on('focus', '.select2', function (e) {
        if (e.originalEvent) {
            $(this).siblings('select').select2('open');
        }
    });
    
    function funopen(url) {
        window.open(url, '_self');
    }
</script>

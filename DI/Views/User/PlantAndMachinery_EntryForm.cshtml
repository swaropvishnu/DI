    <script>
        function myfunction()
        {
            var table = document.getElementById("table");
           // var row = table.insertRow((table.rows.length - 1) + 1);

            if ($("#tab1").hasClass('active'))
            {
                if ((table.rows.length - 1) <= 0) {
                    alert("कृप्या मशीन का विवरण भरे");
                  
                    return;
                }
                if (document.getElementById("txtfixed_deposite").value == "") {
                    document.getElementById("txtfixed_deposite").focus();
                    alert("कुल स्थायी पूंजी विनियोजन");
                    
                }
                if (document.getElementById("txtworking_capital").value == "") {
                    document.getElementById("txtworking_capital").focus();
                    alert("कार्य शील पूंजी (एक चक्र. में )");
                    
                }
                if (document.getElementById("txtprojectcost").value == "") {
                    document.getElementById("txtprojectcost").focus();
                    alert("परियोजना लागत)");

                }
                var tempvar = 0;
                var Plant = [];

                if ((table.rows.length - 1) > 0) {
                    for (i = 0; i < table.rows.length - 1; i++) {
                        if (document.getElementById("txtMachine" + (i + 1) + "").value == "") {
                            alert("मशीन");
                            document.getElementById("txtMachine" + (i + 1) + "").focus();
                            tempvar = 1;
                            break;
                        }
                        if (document.getElementById("txtcost" + (i + 1) + "").value == "") {
                            document.getElementById("txtcost" + (i + 1) + "").focus();
                            alert("कीमत");
                            tempvar = 1;
                            break;
                        }
                        if (document.getElementById("txtsupplier" + (i + 1) + "").value == "") {
                            document.getElementById("txtsupplier" + (i + 1) + "").focus();
                            alert("आपूर्तिकर्ता का नाम");
                            tempvar = 1;
                            break;
                        }
                        
                        Plant.push({ registration_code: document.getElementById("hfapplicant_code").value, machine_name: document.getElementById("txtMachine" + (i + 1) + "").value, supplier: document.getElementById("txtsupplier" + (i + 1) + "").value, price: document.getElementById("txtcost" + (i + 1) + "").value, fixed_deposite: document.getElementById("txtfixed_deposite").value, working_capital: document.getElementById("txtworking_capital").value, project_cost: document.getElementById("txtprojectcost").value, Marketingsystem: document.getElementById('txtMarketingsystem').value })
                    }
                }
                if ( tempvar != 1) {
                    $.ajax({
                        url: '/User/InsertUpdateCMYSS_MachininaryDetails',
                        data: JSON.stringify({ objFamily: Plant }),
                        type: 'POST',
                        contentType: 'application/json; charset=utf-8',
                        success: function (data) {
                            if (data.includes("Save")) {
                                $("#divpross").css('width', '55%');
                                $("#litab2").addClass('active');
                                $("#litab1").removeClass('active');
                                $("#litab1").addClass('done');
                                $("#tab2").addClass('active');
                                $("#tab1").removeClass('active');
                                $("#btnback").removeClass('hide');
                                $("#btncont").html('Submit');
                                // alert(data);
                            }
                            else {
                                alert(data);
                            }
                        }
                    });
                    return;
                }
                


            }
            if ($("#tab2").hasClass('active'))
            {
                var table = document.getElementById("table2");
              //  var row = table.insertRow((table.rows.length - 1) + 1);
                var tempvar = 0;
                if ((table.rows.length - 1) <= 0) {
                    alert("कृप्या पूंजी का विवरण भरे");

                    return;
                }
                if (document.getElementById("txtapproxproduction").value == "") {
                    document.getElementById("txtapproxproduction").focus();
                    alert("कुल अनुमानित उत्पाद ");

                }
                if (document.getElementById("txtapprox_sale").value == "") {

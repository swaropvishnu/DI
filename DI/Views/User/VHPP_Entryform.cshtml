                                            <label class="control-label col-md-12" id="lblplace">स्थान   <span class="Mandatory">*</span></label>
                                            <div class="col-md-12">
                                                @Html.DropDownListFor(m => m.artwork_place, new List<SelectListItem> { new SelectListItem { Text = "--चयन करे--", Value = "-1" } }, new { @id = "ddlplace", @class = "form-control" })



                                                <span class="help-block"></span>
                                            </div>
                                        </div>
                                    </div>


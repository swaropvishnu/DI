@model DI.Models.vhpp_Applicant
@{
    ViewBag.Title = "VHPP_Entryform";
    Layout = "~/Views/Shared/UserLayout.cshtml";
}
@using (Html.BeginForm("VHPP_Entryform", "User", FormMethod.Post, new { @class = "", enctype = "multipart/form-data" }))
{
    @Html.AntiForgeryToken()

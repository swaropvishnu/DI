                                            <th width="15%"> नाम  </th>
                                            <th width="15%"> उम्र </th>
                                            <th width="10%"> कार्य </th>
                                            <th width="10%"> </th>
                                           
                                        </tr>
                                    </thead>
                                    <tbody>
                                        @Html.Raw(Model.OtherRelation)
                                    </tbody>
                                </table>

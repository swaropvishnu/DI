                                            </div>
                                            <div class="col-md-6">
                                                @Html.TextBoxFor(m => m.pancard, new { @class = "form-control", @id = "txtpancard", @placeholder = "पैन कार्ड नं0 ", @Maxlength = 10 })

                                            </div>
                                        </div>
                                    </div>

                                    <div class="col-md-4">
                                        <div class="form-group">
                                            <label class="control col-md-6">मोबाइल नं0<span class="Mandatory">*</span></label><label class="control col-md-6" id="lblpan">शैक्षणिक योग्यता<span class="Mandatory">*</span></label>

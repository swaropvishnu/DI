        function sumcost()
        {
            var tot = 0;
            var table = document.getElementById("tablemachine");
            if ((table.rows.length - 1) > 0)
            {

                for (i = 0; i < table.rows.length - 1; i++)
                {
                    var myEle = document.getElementById("txtcost" + (i + 1) + "");
                    if (myEle) {
                        tot = tot + parseFloat(document.getElementById("txtcost" + (i + 1) + "").value.trim());

                    }
                }
                if (parseFloat(document.getElementById("txtfixed_deposite").value) < tot) {
                    alert("प्लांट एंड मशीनरी की कीमत कुल स्थायी पूंजी विनियोजन से ज्यादा है ।");
                }
                document.getElementById("lbltot").innerHTML = parseFloat(tot).toFixed(2).toString().trim();

            }
        }

        function delmachine(el) {
            //var table = document.getElementById("tablemachine");
            //document.getElementById("tablemachine").deleteRow(id);
            //
            var row = upTo(el, 'tr')
            if (row) row.parentNode.removeChild(row);
            sumcost();
        }
        function upTo(el, tagName) {
            tagName = tagName.toLowerCase();

            while (el && el.parentNode) {
                el = el.parentNode;
                if (el.tagName && el.tagName.toLowerCase() == tagName) {
                    return el;
                }
            }
            return null;
        }


                                    <div class="col-md-4">
                                        <div class="form-group">
                                            <label class="control col-md-12" id="lblname">नाम<span class="Mandatory">*</span></label>
                                            <div class="col-md-12">
                                                @Html.TextBoxFor(m => m.applicant_name, new
                                               {
                                                   @class = "form-control",
                                                   @id = "txtName",
                                                   @placeholder = "नाम ",
                                                   @Maxlength = 20,
                                                   @onkeypress = "return IsAlphaNumeric(event);",
                                                   @ondrop = "return false;",
                                                   @npaste = "return false;"
                                               })
                                                @Html.HiddenFor(m => m.applicant_code, new { @id = "hfapplicant_code" })
                                                @Html.HiddenFor(m => m.steps, new { @id = "hfstep" })
                                                @Html.HiddenFor(m => m.DistCode, new { @id = "hfDistCode" })
                                                @Html.HiddenFor(m => m.TehsilCode, new { @id = "hfTehsilCode" })
                                                @Html.HiddenFor(m => m.BlockCode, new { @id = "hfBlockCode" })
                                                @Html.HiddenFor(m => m.VillCode, new { @id = "hfVillCode" })
                                                @Html.HiddenFor(m => m.current_district_code_census, new { @id = "hfCDistCode" })
                                                @Html.HiddenFor(m => m.current_tehsil_code_census, new { @id = "hfCTehsilCode" })

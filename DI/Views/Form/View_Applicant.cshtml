                                                @Html.HiddenFor(m => m.qualification_code, new { @id = "hfqualification_code" })
                                                @Html.HiddenFor(m => m.category_suffix, new { @id = "hfcategory_suffix" })
                                                @Html.HiddenFor(m => m.sp_category_suffix, new { @id = "hfsp_category_suffix" })
                                                @Html.HiddenFor(m => m.proposed_office_district, new { @id = "hfproposed_office_district" })
                                                @Html.HiddenFor(m => m.proposed_office_tehsil, new { @id = "hfptehsil" })
                                                @Html.HiddenFor(m => m.proposed_office_block, new { @id = "hfpblock" })
                                                @Html.HiddenFor(m => m.proposed_office_village, new { @id = "hfpvillage" })
                                                @Html.HiddenFor(m => m.sponsoring_office_code, new { @id = "hfsponsoring_office_code" })
                                                @Html.HiddenFor(m => m.is_address_same, new { @id = "hfis_address_same", @value = "N" })
                                                @Html.HiddenFor(m => m.industry_code, new { @id = "hfindustry_code" })
                                                @Html.HiddenFor(m => m.Is_village_bank, new { @id = "hfIs_village_bank" })
                                                @Html.HiddenFor(m => m.bank_code, new { @id = "hfbank_code" })
                                                @Html.HiddenFor(m => m.branch_code, new { @id = "hfbranch_code" })
                                            </div>
                                        </div>
                                    </div>
                                    <!--/span-->
                                    <div class="col-md-4">
                                        <div class="form-group">
                                            <label class="control col-md-6" id="lbldob">जन्मतिथि<span class="Mandatory">*</span></label>
                                            <label class="control col-md-6" id="lblgend">लिंग <span class="Mandatory">*</span></label>
                                            <div class="col-md-6">
                                                @Html.EditorFor(model => model.inputdob, new { htmlAttributes = new { @class = "form-control", @onkeypress = "return IsNumeric(event);", @placeholder = "dd/mm/yy", @id = "txtDOB" } })
                                                @Html.ValidationMessageFor(model => model.inputdob, "", new { @class = "text-danger" })

                                            </div>
                                            <div class="col-md-6">
                                                @Html.DropDownListFor(m => m.gender, new List<SelectListItem>
                                                    (), "--चयन करे--", new { @id = "ddlgender", @class = "form-control" })
                                                <span class="help-block"></span>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-4">
                                        <div class="form-group">
                                            <label class="control col-md-6" id="lblcat">जाति <span class="Mandatory">*</span></label>
                                            <label class="control col-md-6" id="lblspcat">विशेष श्रेणी <span class="Mandatory">*</span></label>
                                            <div class="col-md-6">
                                                @Html.DropDownListFor(m => m.category_suffix, new List<SelectListItem>(), "--चयन करे--", new { @id = "ddlcategory", @class = "form-control" })
                                            </div>
                                            <div class="col-md-6">
                                                @Html.DropDownListFor(m => m.sp_category_suffix, new List<SelectListItem>
                                                    (), "--चयन करे--", new { @id = "ddlspcategory", @class = "form-control" })
                                                <span class="help-block"></span>
                                            </div>
                                        </div>
                                    </div>
                                    <!--/span-->
                                </div>
                                <!--/row-->
                                <div class="row">
                                    <div class="col-md-4">
                                        <div class="form-group">
                                            <label class="control col-md-12" id="lblfather">पिता/पति का नाम<span class="Mandatory">*</span></label>
                                            <div class="col-md-12">
                                                @Html.TextBoxFor(m => m.Husband_father_name, new
                                               {
                                                   @class = "form-control",
                                                   @id = "txtHusband_father_name",
                                                   @placeholder = "पिता/पति का नाम ",
                                                   @Maxlength = 20,
                                                   @onkeypress = "return IsAlphaNumeric(event);",
                                                   @ondrop = "return false;",
                                                   @npaste = "return false;"
                                               })
                                            </div>
                                        </div>
                                    </div>
                                    <!--/span-->
                                    <div class="col-md-4">
                                        <div class="form-group">
                                            <label class="control col-md-6" id="lbladhar">आधार कार्ड नं0<span class="Mandatory">*</span></label>

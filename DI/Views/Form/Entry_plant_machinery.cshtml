                                    <div class="col-md-4">
                                        <div class="form-group">
                                            <label class="control-label col-md-12">1.लाभार्थी  का नाम </label>
                                            <div class="col-md-12">
                                                @Html.TextBoxFor(m => m.applicant_name, new { @class = "form-control", @id = "txtName", @placeholder = "लाभार्थी  का नाम", @Maxlength = 20, disabled = "disabled" })
                                                
                                            </div>
                                        </div>
                                    </div>
                                    <!--/span-->
                                    <div class="col-md-4">
                                        <div class="form-group">
                                            <label class="control-label col-md-12">2.पिता/पति का नाम</label>
                                            <div class="col-md-12">
                                                @Html.TextBoxFor(m => m.father_name, new { @class = "form-control", @id = "txtFather_Name", @placeholder = "पिता/पति का नाम ", @Maxlength = 20, disabled = "disabled" })                                           
                                            </div>
                                        </div>
                                    </div>

                                    <div class="col-md-4">
                                        <div class="form-group">
                                            <label class="control-label col-md-12">3. पता</label>
                                            <div class="col-md-12">
                                                @Html.TextBoxFor(m => m.address, new { @class = "form-control", @id = "txtAddress", @placeholder = "पता", @Maxlength = 200, disabled = "disabled" })           


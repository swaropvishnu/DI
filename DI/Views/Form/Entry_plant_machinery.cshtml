                                            <div class="col-md-12">
                                                @Html.TextBoxFor(m => m.office_address, new { @class = "form-control", @id = "txt_office_Address", @placeholder = "प्रस्तावित कार्यालय का पता", @Maxlength = 500, disabled = "disabled" })
                                                <span class="help-block"></span>

@Styles.Render("~/Content/ReportCss")
<!-- BEGIN EXAMPLE TABLE PORTLET-->
<div class="portlet box green">
    <div class="portlet-title red">
        <div class="caption">
            <i class="fa fa-globe"></i>Entry Report
        </div>
        <div class="tools"> </div>
    </div>
    <div class="portlet-body">
        <table class="table table-striped table-bordered table-hover" id="Datatable">
            <thead>
                <tr>
                    <th> प्रार्थी का नाम</th>
                    <th> प्रार्थी का पता </th>
                    <th>औद्योगिक इकाई का नाम  </th>
                    <th>प्रार्थना-पत्र शुल्क </th>
                    <th> शुल्क दिनांक</th>
                    <th> शुल्क विवरण</th>
                    <th> उत्पादित सामग्री </th>
                    <th> प्रोजेक्ट रिपोर्ट व्यावहारिक</th>
                    <th>विदेशी सहायता</th>
                    <th>वित्तीय सहायता का श्रोत</th>
                    <th>कच्चे माल का श्रोत</th>
                </tr>
            </thead>
            <tbody>
               @foreach (var item in Model)
               {
                   
                <tr>

                    <td>
                        @Html.DisplayFor(Model => item.ApplicantName)
                    </td>
                    <td>
                        @Html.DisplayFor(Model => item.ApplicantAddress)
                    </td>
                    <td>
                        @Html.DisplayFor(Model => item.IndrustryName)
                    </td>
                    <td>
                        @Html.DisplayFor(Model => item.ApplicationFee)
                    </td>
                    <td>
                        @Html.DisplayFor(Model => item.ApplicationFeeDate)
                    </td>
                    <td>
                        @Html.DisplayFor(Model => item.ApplicationFeedetails)
                    </td>
                    <td>
                        @Html.DisplayFor(Model => item.ProductName)
                    </td>
                  
                    
                    <td>
                        @Html.DisplayFor(Model => item.HelpFromForegin)
                    </td>
                    <td>
                        @Html.DisplayFor(Model => item.PracticalProjectReport)
                    </td>
                    <td>
                        @Html.DisplayFor(Model => item.FiananceDetails)
                    </td>
                    <td>
                        @Html.DisplayFor(Model => item.RowMeterialSource)
                    </td>
                    @*<td>
                        @Html.ActionLink("Edit", "Entry", new { id = item.application }) |
                        @Html.ActionLink("Delete", "DeleteSociety", new { id = item.Id })
                    </td>*@
                </tr>
               }
            </tbody>
        </table>
    </div>
</div>

<!-- END EXAMPLE TABLE PORTLET-->

        <div class="col-lg-3 col-md-3 col-sm-6 col-xs-12">
            <a href="@Url.Action("MYSY_Applicant_status_report", "Report",new {A=new DI.Crypto().Encrypt("Z")})">
                <div class="dashboard-stat2 ">
                    <div class="display">
                        <div class="number">
                            <h3 class="font-green-sharp">
                                <span data-counter="counterup" data-value="@dt.Rows[0]["totalMYSYApplicant"].ToString().Trim()">@dt.Rows[0]["totalMYSYApplicant"].ToString().Trim()</span>
                                <small class="font-green-sharp"></small>
                            </h3>
                            <small>Total Applicant in MYSY</small>
                        </div>
                        <div class="icon">
                            <i class="icon-pie-chart"></i>
                        </div>
                    </div>
                    <div class="progress-info">

                        <div class="progress">
                            <span style="width: @dt.Rows[0]["PerApplicant"].ToString().Trim()%;" class="progress-bar progress-bar-success green-sharp">
                                <span class="sr-only">@dt.Rows[0]["PerApplicant"].ToString().Trim()% </span>
                            </span>
                        </div>
                        <div class="status">
                            <div class="status-title"> Complete Submission </div>
                            <div class="status-number">@dt.Rows[0]["PerApplicant"].ToString().Trim()% </div>
                        </div>
                    </div>
                </div>
            </a>
        </div>

